.brand 
    .container 
        .row
            -let imgs = ['01','02','03','04']
            -let brandCol = ['col-lg-3','col-md-6','col-12']
            each img in imgs
                div(class=brandCol)
                    div(class="brand-item")
                        img(src=`./images/brand-${img}.png`, alt="brand-img")
                    